{
  "name": "@asos/api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"pnpm dev:api\" \"pnpm dev:ui\"",
    "dev:api": "tsx watch src/server.ts",
    "dev:ui": "vite",
    "build": "vite build && tsc -p tsconfig.build.json",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@fastify/static": "^9.0.0",
    "fastify": "^5.7.1",
    "prom-client": "^15.1.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}